<form class="{{styles.form}}">
  {{#each inputs}}
    {{> Input
          inputId=this.inputId
          name=this.name
          placeholder=this.placeholder
          label=this.label
          value=this.value
          styles=../inputStyles
          type=this.type
    }}
  {{/each}}

  {{> Button 
        type="submit"
        text="Sign in"
        size="l"
        variant="primary"
        block=true
        styles=btnStyles
  }}
</form>